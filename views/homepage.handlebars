<!-- Loop over each object in the projects array -->
{{#each TechBlogs as |TechBlog| }}
<div class="row mb-4 techblog">
  <div class="col-md-5">
    <h2>
      <!-- Call helper function with triple brackets to render the emoji span as HTML -->
      {{{get_emoji}}}
      <a href="/techblog/{{TechBlog.id}}">{{TechBlog.title}}</a>
    </h2>
    
    <p>Created by {{TechBlog.user.name}} on {{format_date TechBlog.date_created}}</p>
  </div>
  <div class="col-md-7">
    <p>
      {{TechBlog.contents}}
    </p>
  </div>
</div>
{{/each}}
